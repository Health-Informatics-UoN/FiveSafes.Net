<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>FiveSafes</RootNamespace>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageTags>FiveSafes 5Safes fivesafes 5safes FiveSafes.Net</PackageTags>
    <PackageId>FiveSafes.Net</PackageId>
    <PackageVersion>0.1.0</PackageVersion>
    <Authors>Daniel Lea</Authors>
    <Company>Digital Research Service</Company>
    <RepositoryUrl>https://github.com/Health-Informatics-UoN/FiveSafes.Net.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageProjectUrl>https://github.com/Health-Informatics-UoN/FiveSafes.Net</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ROCrates.Net" Version="0.1.4"/>
  </ItemGroup>

</Project>
